{
  "name": "@monorepo-fullstack-app/source",
  "version": "1.0.0",
  "private": false,
  "description": "Example NX monorepo with next.js as frontend, nest.js as backend and gitlab ci/cd",
  "author": "Anatoly Chesnokov",
  "repository": {
    "type": "git",
    "url": "https://github.com/AnatolyChesnokov/nx-monorepo-next-nest-gitlab-ci.git"
  },
  "keywords": [
    "nx",
    "monorepo",
    "next",
    "nest",
    "prisma",
    "ory",
    "postgres",
    "gitlab",
    "ci",
    "cd",
    "docker",
    "pnpm",
    "opentelemetry",
    "fastify",
    "starter",
    "boilerplate",
    "backend",
    "frontend",
    "affected",
    "nx-cloud"
  ],
  "scripts": {
    "build": "nx run-many --target=build --all=true",
    "build:api": "nx run api:build",
    "build:www": "nx run www:build",
    "build:docker:api": "docker build -f apps/api/Dockerfile .",
    "build:docker:www": "docker build -f apps/www/Dockerfile .",
    "dev:api": "nx run api:serve:development --watch=true",
    "dev:www": "nx run www:serve-static:development --watch=true",
    "dev:docker": "docker compose -f docker-compose.base.yaml -f docker-compose.dev.yaml",
    "dev:docker:up": "pnpm dev:docker up -d",
    "dev:docker:down": "pnpm dev:docker down",
    "prisma:generate": "rimraf packages/types/src/lib/generated && prisma generate",
    "migrate:deploy": "prisma migrate deploy",
    "migrate:dev": "prisma migrate dev",
    "migrate:reset": "rimraf packages/types/src/lib/generated && prisma migrate reset --force",
    "db:push": "prisma db push",
    "postinstall": "pnpm prisma:generate"
  },
